journal = {Agriculture, Ecosystems and Environment},
keywords = {Bio-char,Biomass,Carbon,Charcoal,Renewable,Soil,Western Australia},
number = {1-3},
pages = {1--7},
title = {{Agricultural bio-char production, renewable energy generation and farm carbon sequestration in Western Australia: Certainty, uncertainty and risk}},
volume = {129},
year = {2009}
}


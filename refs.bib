journal = {HortScience},
keywords = {Evapotranspiration,Microirrigation,Prunus persica,Soil water content,Stem water potential,Water relations},
number = {7},
pages = {2118--2124},
title = {{Influence of irrigation method and scheduling on patterns of soil and tree water status and its relation to yield and fruit quality in peach}},
volume = {40},
